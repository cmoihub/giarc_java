from math import sin
from scipy.stats.vonmises_cython import numpy
import random as r

def main():
    a = 1
    b = 1
    count = 0
    for i in range (0, 10):
        y_ = "Y = "
        y = r.uniform(0, a)
        o_ = " Angle = "
        o = r.uniform(0, numpy.pi)
        limit = b * sin(o)
        print("{}{}{}{}".format(y_, y, o_, o))
        if(y < limit):
            count += 1
    print count


main()
