from cmath import sin
from scipy.stats.vonmises_cython import numpy
import random as r

def main():
    a = 4
    b = numpy.pi
    o = r.uniform(0, numpy.pi)
    limit = b * sin(o)
    y = r.uniform(0, a)
    count = 0
    for i in range (0, 50):
        print(y)
        print(o)
        print(i)
        if(y < b):
            count + 1
    print count


main()
