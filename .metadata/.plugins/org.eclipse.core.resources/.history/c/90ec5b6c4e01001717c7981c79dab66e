package ass4;

public class BSTDictionary<E,K extends Sortable> implements Dictionary<E,K> {

	BSTNode <E, K> root;
	BSTNode <E, K> curr;
	
	public BSTDictionary(){
		root = null;
	}//Default constructor
	
	@Override
	public E search(K key) {
//		if(curr == null)//gone through whole tree and did not find element
//		{
//			curr = root;
//			return null;
//		}
		
		if(curr.getKey().compareTo(key) == 0){
			return curr.getElement();
		}//if key is the one at the current node
		
		else if(curr.getKey().compareTo(key) < 0){
			return search(curr.getLeft().getKey());
		}//left child check
		
		else/* if(curr.getKey().compareTo(key) > 0)*/{
			return search(curr.getRight().getKey());
		}//right child check
	}

	@Override
	public void insert(K key, E element) {
		
	}

	@Override
	public void delete(K key) {
		
	}

	@Override
	public void printTree() {
		
	}

	@Override
	public int depth() {

		return 0;
	}

}
