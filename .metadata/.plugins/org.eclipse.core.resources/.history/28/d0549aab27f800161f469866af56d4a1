package Assignment2;

import Assignment2.TestListReferenceBased.Node;

public class Node <T> {
	private final T data;
    private Node<T> next;

    public Node(T data) {
        this.data = data;
    }
    
    public Node (T data, Node<T> next){
    	this.data = data;
    	this.next = next;
    }

    @Override
    public String toString() {
        return data.toString();
    }
    
    Object getNodeContent(){return data;}
	void setNodeContents(T newData){data = newData;}
	Node getNextNode(){return next;}
	void setNextNode(Node next){this.next = next;}
}
